class ClientComponent {
    constructor(clientService, alertsService, reloadPageService) {
        this.clientService = clientService;
        this.alertsService = alertsService;
        this.reloadPageService = reloadPageService;
        this.clients = [];
        this.errorMessage = '';
        this.isClientAlreadyExists = false;
    }
    ngOnInit() {
        this.getClients();
        this.formClient();
    }
    formClient() {
        this.addClientForm = new _angular_forms__WEBPACK_IMPORTED_MODULE_1__["FormGroup"]({
            firstName: new _angular_forms__WEBPACK_IMPORTED_MODULE_1__["FormControl"](''),
            lastName: new _angular_forms__WEBPACK_IMPORTED_MODULE_1__["FormControl"](''),
            email: new _angular_forms__WEBPACK_IMPORTED_MODULE_1__["FormControl"](''),
            phoneNumber: new _angular_forms__WEBPACK_IMPORTED_MODULE_1__["FormControl"]('')
        });
    }
    editFormClient(modalDirective, client) {
        modalDirective.toggle();
        this.editClientForm = new _angular_forms__WEBPACK_IMPORTED_MODULE_1__["FormGroup"]({
            id: new _angular_forms__WEBPACK_IMPORTED_MODULE_1__["FormControl"](client.id),
            firstName: new _angular_forms__WEBPACK_IMPORTED_MODULE_1__["FormControl"](client.firstName),
            lastName: new _angular_forms__WEBPACK_IMPORTED_MODULE_1__["FormControl"](client.lastName),
            email: new _angular_forms__WEBPACK_IMPORTED_MODULE_1__["FormControl"](client.email),
            phoneNumber: new _angular_forms__WEBPACK_IMPORTED_MODULE_1__["FormControl"](client.phoneNumber)
        });
    }
    getClients() {
        this.clientService.getAllClients().subscribe((data) => {
            this.clients = data;
        }, err => {
            this.clients = JSON.parse(err.message).message;
        });
    }
    addClient(modalDirective) {
        this.clientService.addClient(this.addClientForm.value).subscribe(response => {
            this.alertsService.success();
            this.isClientAlreadyExists = false;
            modalDirective.toggle();
            this.reloadPageService.reload();
        }, err => {
            this.errorMessage = err.error.message;
            this.isClientAlreadyExists = true;
        });
    }
    updateClient(modalDirective) {
        const index = this.clients.find(client => client.id == this.editClientForm.value.id);
        this.clients[index] = this.editClientForm.value;
        const id = this.clients[index].id;
        this.clientService.editClientById(id, this.clients[index]).subscribe(response => {
            this.alertsService.success();
            modalDirective.toggle();
            this.reloadPageService.reload();
            this.isClientAlreadyExists = false;
        }, err => {
            this.errorMessage = err.error.message;
            this.isClientAlreadyExists = true;
        });
    }
    deleteClient(client) {
        sweetalert__WEBPACK_IMPORTED_MODULE_2___default()({
            title: 'Are you sure?',
            text: 'Once deleted, you will not be able to recover this registration!',
            icon: 'warning',
            buttons: ['Cancel', 'Ok'],
            dangerMode: true,
        })
            .then((willDelete) => {
            if (willDelete) {
                this.clientService.deleteClientById(client.id).subscribe(response => {
                    this.reloadPageService.reload();
                }, error => {
                    console.log(error);
                });
                sweetalert__WEBPACK_IMPORTED_MODULE_2___default()('Your registration/file has been deleted!', {
                    icon: 'success',
                });
            }
        });
    }
}
