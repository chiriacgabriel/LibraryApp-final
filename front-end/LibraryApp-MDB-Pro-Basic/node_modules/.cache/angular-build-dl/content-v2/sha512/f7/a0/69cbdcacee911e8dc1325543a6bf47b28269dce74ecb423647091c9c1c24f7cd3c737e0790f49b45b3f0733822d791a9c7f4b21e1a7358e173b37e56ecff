-->\n\n    <!--Grid column-->\n    <div class=\"col-xl-3 col-md-6 mb-r\">\n\n      <!--Card-->\n      <mdb-card cascade=\"true\" class=\"cascading-admin-card\">\n\n        <!--Card Data-->\n        <div class=\"admin-up\">\n          <mdb-icon fas icon=\"users\" class=\"warning-color\"></mdb-icon>\n          <div class=\"data\">\n            <p style=\"font-size: 20px\">USERS</p>\n            <h4><strong>{{countUsers}}</strong></h4>\n          </div>\n        </div>\n        <!--/.Card Data-->\n\n        <!--Card content-->\n        <mdb-card-body>\n\n        </mdb-card-body>\n        <!--/.Card content-->\n\n      </mdb-card>\n      <!--/.Card-->\n\n    </div>\n    <!--Grid column-->\n\n    <!--Grid column-->\n    <div class=\"col-xl-3 col-md-6 mb-r\">\n\n      <!--Card-->\n      <mdb-card cascade=\"true\" class=\"cascading-admin-card\">\n\n        <!--Card Data-->\n        <div class=\"admin-up\">\n          <mdb-icon fas icon=\"book-open\" class=\"light-blue lighten-1\"></mdb-icon>\n          <div class=\"data\">\n            <p style=\"font-size: 20px\">BOOKS</p>\n            <h4><strong>{{countBooks}}</strong></h4>\n          </div>\n        </div>\n        <!--/.Card Data-->\n\n        <!--Card content-->\n        <mdb-card-body>\n\n        </mdb-card-body>\n        <!--/.Card content-->\n\n      </mdb-card>\n      <!--/.Card-->\n\n    </div>\n    <!--Grid column-->\n\n    <!--Grid column-->\n    <div class=\"col-xl-3 col-md-6 mb-r\">\n\n      <!--Card-->\n      <mdb-card cascade=\"true\" class=\"cascading-admin-card\">\n\n        <!--Card Data-->\n        <div class=\"admin-up\">\n          <mdb-icon fas icon=\"calendar-check\" class=\"red accent-2\"></mdb-icon>\n          <div class=\"data\">\n            <p style=\"font-size: 20px\">RESERVATIONS</p>\n            <h4><strong>{{countReservations}}</strong></h4>\n          </div>\n        </div>\n        <!--/.Card Data-->\n\n        <!--Card content-->\n        <mdb-card-body>\n\n        </mdb-card-body>\n   