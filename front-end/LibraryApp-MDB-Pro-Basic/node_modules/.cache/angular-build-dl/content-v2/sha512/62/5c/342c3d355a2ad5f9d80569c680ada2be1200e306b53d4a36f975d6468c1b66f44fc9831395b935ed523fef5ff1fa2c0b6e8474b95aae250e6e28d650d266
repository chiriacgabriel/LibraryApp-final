class AlertsService {
    constructor(toast) {
        this.toast = toast;
    }
    success() {
        const options = { timeOut: 1500 };
        this.toast.success('Action performed successfully', '', options);
    }
    warning() {
        const options = { timeOut: 1500 };
        this.toast.warning('Delete was successful', '', options);
    }
}
