class AuthorComponent {
    constructor(authorService, authorImageUrl, alertsService, reloadPageService, sanitizer, router) {
        this.authorService = authorService;
        this.authorImageUrl = authorImageUrl;
        this.alertsService = alertsService;
        this.reloadPageService = reloadPageService;
        this.sanitizer = sanitizer;
        this.router = router;
        this.authors = [];
        this.imageUrlList = [];
        this.errorMessageAddAuthor = '';
        this.isAuthorAlreadyExist = false;
    }
    ngOnInit() {
        this.getAuthors();
        this.getImageUrlAuthors();
        this.formAuthor();
        this.dateForm = new _angular_forms__WEBPACK_IMPORTED_MODULE_1__["FormGroup"]({
            dateBirth: new _angular_forms__WEBPACK_IMPORTED_MODULE_1__["FormControl"](''),
            deathDate: new _angular_forms__WEBPACK_IMPORTED_MODULE_1__["FormControl"]('')
        });
    }
    getAuthors() {
        this.authorService.getAllAuthors().subscribe((data) => {
            this.authors = data;
            this.authors.forEach((image) => {
                image.authorImageUrl.imageUrl = this.sanitizer.bypassSecurityTrustResourceUrl(`data:image/png;base64, ${image.authorImageUrl.imageUrl}`);
            });
        }, error => {
            this.authors = JSON.parse(error.message).message;
        });
    }
    getImageUrlAuthors() {
        this.authorImageUrl.getAllImages().subscribe((data) => {
            this.imageUrlList = data;
        }, error => {
            this.imageUrlList = JSON.parse(error.message).message;
        });
    }
    formAuthor() {
        this.addAuthorForm = new _angular_forms__WEBPACK_IMPORTED_MODULE_1__["FormGroup"]({
            firstName: new _angular_forms__WEBPACK_IMPORTED_MODULE_1__["FormControl"](''),
            lastName: new _angular_forms__WEBPACK_IMPORTED_MODULE_1__["FormControl"](''),
            dateOfBirth: new _angular_forms__WEBPACK_IMPORTED_MODULE_1__["FormControl"](''),
            nationality: new _angular_forms__WEBPACK_IMPORTED_MODULE_1__["FormControl"](''),
            description: new _angular_forms__WEBPACK_IMPORTED_MODULE_1__["FormControl"](''),
            type: new _angular_forms__WEBPACK_IMPORTED_MODULE_1__["FormControl"](''),
            authorImageUrl: new _angular_forms__WEBPACK_IMPORTED_MODULE_1__["FormControl"]('')
        });
    }
    editFormAuthor(author, modalDirective) {
        const authorImageSelected = this.imageUrlList.find(i => i.id == author.authorImageUrl.id);
        modalDirective.toggle();
        this.editAuthorForm = new _angular_forms__WEBPACK_IMPORTED_MODULE_1__["FormGroup"]({
            id: new _angular_forms__WEBPACK_IMPORTED_MODULE_1__["FormControl"](author.id),
            firstName: new _angular_forms__WEBPACK_IMPORTED_MODULE_1__["FormControl"](author.firstName),
            lastName: new _angular_forms__WEBPACK_IMPORTED_MODULE_1__["FormControl"](author.lastName),
            dateOfBirth: new _angular_forms__WEBPACK_IMPORTED_MODULE_1__["FormControl"](author.dateOfBirth),
            nationality: new _angular_forms__WEBPACK_IMPORTED_MODULE_1__["FormControl"](author.nationality),
            description: new _angular_forms__WEBPACK_IMPORTED_MODULE_1__["FormControl"](author.description),
            type: new _angular_forms__WEBPACK_IMPORTED_MODULE_1__["FormControl"](author.type),
            bookList: new _angular_forms__WEBPACK_IMPORTED_MODULE_1__["FormControl"](author.bookSet),
            authorImageUrl: new _angular_forms__WEBPACK_IMPORTED_MODULE_1__["FormControl"](author.authorImageUrl)
        });
        this.editAuthorForm.get('authorImageUrl').setValue(authorImageSelected);
    }
    addAuthor(modalDirective) {
        // tslint:disable-next-line:max-line-length
        this.addAuthorForm.controls.dateOfBirth.setValue(this.dateForm.controls.dateBirth.value + ' - ' + this.dateForm.controls.deathDate.value);
        if (this.addAuthorForm.value.authorImageUrl == '') {
            this.addAuthorForm.get('authorImageUrl').setValue(null);
        }
        this.authorService.addAuthor(this.addAuthorForm.value).subscribe(response => {
            this.isAuthorAlreadyExist = false;
            this.alertsService.success();
            modalDirective.toggle();
            this.reloadPageService.reload();
        }, err => {
            this.errorMessageAddAuthor = err.error.message;
            this.isAuthorAlreadyExist = true;
        });
    }
    updateAuthor(modalDirective) {
        const index = this.authors.findIndex(author => author.id == this.editAuthorForm.value.id);
        this.authors[index] = this.editAuthorForm.value;
        const id = this.authors[index].id;
        this.authorService.editAuthorById(id, this.authors[index]).subscribe(response => {
            this.alertsService.success();
            this.isAuthorAlreadyExist = false;
            modalDirective.toggle();
            this.reloadPageService.reload();
        }, err => {
            this.errorMessageAddAuthor = err.error.message;
            this.isAuthorAlreadyExist = true;
        });
    }
    deleteAuthor(author) {
        sweetalert__WEBPACK_IMPORTED_MODULE_2___default()({
            title: 'Are you sure?',
            text: 'Once deleted, you will not be able to recover this registration!',
            icon: 'warning',
            buttons: ['Cancel', 'Ok'],
            dangerMode: true,
        })
            .then((willDelete) => {
            if (willDelete) {
                this.authorService.deleteAuthorById(Number(author.id)).subscribe(response => {
                    this.reloadPageService.reload();
                }, error => {
                    console.log(error);
                });
                sweetalert__WEBPACK_IMPORTED_MODULE_2___default()('Your registration/file has been deleted!', {
                    icon: 'success',
                });
            }
        });
    }
    sendToAuthorInfo(author) {
        this.router.navigateByUrl('dashboard/author/' + author.id);
    }
}
