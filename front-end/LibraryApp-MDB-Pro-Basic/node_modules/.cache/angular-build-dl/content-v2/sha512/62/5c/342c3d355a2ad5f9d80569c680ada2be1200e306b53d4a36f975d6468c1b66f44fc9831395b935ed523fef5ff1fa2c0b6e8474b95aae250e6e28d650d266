const _c0 = function () { return ["/dashboard/author"]; };
class AuthorDetailsComponent {
    constructor(authorService, activatedRoute, sanitizer) {
        this.authorService = authorService;
        this.activatedRoute = activatedRoute;
        this.sanitizer = sanitizer;
        this.author = new _model_Author__WEBPACK_IMPORTED_MODULE_1__["Author"]();
        this.author.authorImageUrl = new _model_AuthorImageUrl__WEBPACK_IMPORTED_MODULE_2__["AuthorImageUrl"]();
    }
    ngOnInit() {
        const id = this.activatedRoute.snapshot.paramMap.get('id');
        this.authorService.getAuthorById(Number(id)).subscribe(author => {
            this.author = author;
            this.author.authorImageUrl.imageUrl = this.sanitizer.bypassSecurityTrustResourceUrl(`data:image/png;base64, ${this.author.authorImageUrl.imageUrl}`);
            this.author.bookSet.forEach((image) => {
                image.bookImageUrl.imageUrl = this.sanitizer.bypassSecurityTrustResourceUrl(`data:image/png;base64, ${image.bookImageUrl.imageUrl}`);
            });
        });
