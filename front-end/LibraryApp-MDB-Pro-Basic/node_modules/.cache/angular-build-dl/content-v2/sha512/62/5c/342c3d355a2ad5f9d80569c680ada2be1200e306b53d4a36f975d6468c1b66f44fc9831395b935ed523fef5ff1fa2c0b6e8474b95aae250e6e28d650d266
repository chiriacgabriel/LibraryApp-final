class ReloadPageService {
    constructor(router) {
        this.router = router;
    }
    reload() {
        const currentUrl = this.router.url;
        this.router.navigateByUrl('/', { skipLocationChange: true }).then(() => {
            this.router.navigate([currentUrl]);
        });
    }
}
