class BookComponent {
    constructor(bookImageUrlService, bookCategoryTypeService, bookService, authorService, alertsService, reloadPageService, sanitizer) {
        this.bookImageUrlService = bookImageUrlService;
        this.bookCategoryTypeService = bookCategoryTypeService;
        this.bookService = bookService;
        this.authorService = authorService;
        this.alertsService = alertsService;
        this.reloadPageService = reloadPageService;
        this.sanitizer = sanitizer;
        this.bookImages = [];
        this.bookCategories = [];
        this.fictionals = [];
        this.nonfictionals = [];
        this.books = [];
        this.authors = [];
        this.errorMessage = '';
        this.isErrorSelected = false;
        this.isFictional = false;
        this.isNonfictional = false;
        this.preSelectedTitle = '';
    }
    ngOnInit() {
        this.getAllBookImages();
        this.getAllBooks();
        this.getAllAuthors();
        this.getAllBookCategories();
        this.getAllFictionals();
        this.getAllNonfictionals();
        this.formBook();
    }
    formBook() {
        this.addBookForm = new _angular_forms__WEBPACK_IMPORTED_MODULE_1__["FormGroup"]({
            title: new _angular_forms__WEBPACK_IMPORTED_MODULE_1__["FormControl"](''),
            author: new _angular_forms__WEBPACK_IMPORTED_MODULE_1__["FormControl"](''),
            stock: new _angular_forms__WEBPACK_IMPORTED_MODULE_1__["FormControl"]('', _angular_forms__WEBPACK_IMPORTED_MODULE_1__["Validators"].required),
            bookImageUrl: new _angular_forms__WEBPACK_IMPORTED_MODULE_1__["FormControl"](''),
            bookCategory: new _angular_forms__WEBPACK_IMPORTED_MODULE_1__["FormControl"](''),
            fictional: new _angular_forms__WEBPACK_IMPORTED_MODULE_1__["FormControl"](),
            nonfictional: new _angular_forms__WEBPACK_IMPORTED_MODULE_1__["FormControl"]()
        });
    }
    editFormBook(book, modalDirective) {
        const bookImageSelected = this.bookImages.find(i => i.id == book.bookImageUrl.id);
        const bookCategorySelected = this.bookCategories.find(i => i.id == book.bookCategory.id);
        const authorSelected = this.authors.find(i => i.id == book.author.id);
        this.editBookForm = new _angular_forms__WEBPACK_IMPORTED_MODULE_1__["FormGroup"]({
            id: new _angular_forms__WEBPACK_IMPORTED_MODULE_1__["FormControl"](book.id),
            title: new _angular_forms__WEBPACK_IMPORTED_MODULE_1__["FormControl"](book.title),
            author: new _angular_forms__WEBPACK_IMPORTED_MODULE_1__["FormControl"](book.author),
            stock: new _angular_forms__WEBPACK_IMPORTED_MODULE_1__["FormControl"](book.stock),
            bookImageUrl: new _angular_forms__WEBPACK_IMPORTED_MODULE_1__["FormControl"](book.bookImageUrl),
            bookCategory: new _angular_forms__WEBPACK_IMPORTED_MODULE_1__["FormControl"](book.bookCategory),
            fictional: new _angular_forms__WEBPACK_IMPORTED_MODULE_1__["FormControl"](book.fictional),
            nonfictional: new _angular_forms__WEBPACK_IMPORTED_MODULE_1__["FormControl"](book.nonfictional)
        });
        this.editBookForm.get('author').setValue(authorSelected);
        this.editBookForm.get('bookImageUrl').setValue(bookImageSelected);
        this.editBookForm.get('bookCategory').setValue(bookCategorySelected);
        if (book.fictional != null) {
            const fictionalSelected = this.fictionals.find(i => i.id == book.fictional.id);
            this.editBookForm.get('fictional').setValue(fictionalSelected);
            this.isFictional = true;
            this.isNonfictional = false;
        }
        if (book.nonfictional != null) {
            const nonfictionalSelected = this.nonfictionals.find(i => i.id == book.nonfictional.id);
            this.editBookForm.get('nonfictional').setValue(nonfictionalSelected);
            this.isNonfictional = true;
            this.isFictional = false;
        }
        modalDirective.toggle();
    }
    getAllBookImages() {
        this.bookImageUrlService.getAllImageBook().subscribe((data) => {
            this.bookImages = data;
        }, err => {
            this.bookImages = JSON.parse(err.message).message;
        });
    }
    getAllAuthors() {
        this.authorService.getAllAuthors().subscribe((data) => {
            this.authors = data;
        }, error => {
            this.authors = JSON.parse(error.message).message;
        });
    }
    getAllBooks() {
        this.bookService.getAllBooks().subscribe((data) => {
            this.books = data;
            this.books.forEach((imageBook) => {
                imageBook.bookImageUrl.imageUrl = this.sanitizer.bypassSecurityTrustResourceUrl(`data:image/png;base64, ${imageBook.bookImageUrl.imageUrl}`);
            });
        }, error => {
            this.books = JSON.parse(error.message).message;
        });
    }
    getAllBookCategories() {
        this.bookCategoryTypeService.getAllBookCategories().subscribe((data) => {
            this.bookCategories = data;
        }, error => {
            this.bookCategories = JSON.parse(error.message).message;
        });
    }
    getAllFictionals() {
        this.bookCategoryTypeService.getAllFictional().subscribe((data) => {
            this.fictionals = data;
        }, error => {
            this.fictionals = JSON.parse(error.message).message;
        });
    }
    getAllNonfictionals() {
        this.bookCategoryTypeService.getAllNonfictional().subscribe((data) => {
            this.nonfictionals = data;
        }, error => {
            this.nonfictionals = JSON.parse(error.message).message;
        });
    }
    addBook(modalDirective) {
        if (this.addBookForm.value.author == '') {
            this.addBookForm.get('author').setValue(null);
        }
        if (this.addBookForm.value.bookCategory == '') {
            this.addBookForm.get('bookCategory').setValue(null);
        }
        this.bookService.addBook(this.addBookForm.value).subscribe(response => {
            this.reloadPageService.reload();
            this.alertsService.success();
            modalDirective.toggle();
            this.isNonfictional = false;
            this.isFictional = false;
            this.isErrorSelected = false;
        }, error => {
            this.errorMessage = error.error.message;
            this.isErrorSelected = true;
        });
    }
    updateBook(modalDirective) {
        const index = this.books.findIndex(book => book.id == this.editBookForm.value.id);
        this.books[index] = this.editBookForm.value;
        const id = this.books[index].id;
        this.bookService.editBookById(id, this.books[index]).subscribe(response => {
            this.reloadPageService.reload();
            this.alertsService.success();
            modalDirective.toggle();
        }, err => {
            console.log(err);
        });
    }
    deleteBook(book) {
        sweetalert__WEBPACK_IMPORTED_MODULE_2___default()({
            title: 'Are you sure?',
            text: 'Once deleted, you will not be able to recover this registration!',
            icon: 'warning',
            buttons: ['Cancel', 'Ok'],
            dangerMode: true,
        })
            .then((willDelete) => {
            if (willDelete) {
                this.bookService.deleteBookById(Number(book.id)).subscribe(response => {
                    this.reloadPageService.reload();
                    this.alertsService.warning();
                }, error => {
                    console.log(error);
                });
                sweetalert__WEBPACK_IMPORTED_MODULE_2___default()('Your registration/file has been deleted!', {
                    icon: 'success',
                });
            }
        });
    }
    isFictionalSelected(bookCategory) {
        if (bookCategory.nameOfBookCategory == 'Fiction') {
            this.isFictional = true;
            this.isNonfictional = false;
        }
        if (bookCategory.nameOfBookCategory == 'Nonfiction') {
            this.isFictional = false;
            this.isNonfictional = true;
        }
    }
    getPreSelectedTitle(bookImageUrl) {
        this.preSelectedTitle = bookImageUrl.title;
    }
}
