class RegisterComponent {
    constructor(authService, modalRefRegister, modalService) {
        this.authService = authService;
        this.modalRefRegister = modalRefRegister;
        this.modalService = modalService;
        this.form = {};
        this.isSuccessful = false;
        this.isSignUpFailed = false;
        this.errorMessage = '';
    }
    ngOnInit() {
        this.validatingForm = new _angular_forms__WEBPACK_IMPORTED_MODULE_1__["FormGroup"]({
            modalFormElegantEmail: new _angular_forms__WEBPACK_IMPORTED_MODULE_1__["FormControl"]('', _angular_forms__WEBPACK_IMPORTED_MODULE_1__["Validators"].email),
            modalFormElegantPassword: new _angular_forms__WEBPACK_IMPORTED_MODULE_1__["FormControl"]('', _angular_forms__WEBPACK_IMPORTED_MODULE_1__["Validators"].required),
            modalFormElegantName: new _angular_forms__WEBPACK_IMPORTED_MODULE_1__["FormControl"]('', _angular_forms__WEBPACK_IMPORTED_MODULE_1__["Validators"].required),
            modalFormElegantLastName: new _angular_forms__WEBPACK_IMPORTED_MODULE_1__["FormControl"]('', _angular_forms__WEBPACK_IMPORTED_MODULE_1__["Validators"].required)
        });
    }
    onSubmit() {
        this.authService.register(this.form).subscribe(data => {
            this.isSuccessful = true;
            this.isSignUpFailed = false;
            this.modalRefRegister.hide();
            this.modalService.show(_login_login_component__WEBPACK_IMPORTED_MODULE_2__["LoginComponent"]);
        }, err => {
            this.errorMessage = err.error.message;
            this.isSignUpFailed = true;
        });
    }
    get modalFormElegantEmail() {
        return this.validatingForm.get('modalFormElegantEmail');
    }
    get modalFormElegantPassword() {
        return this.validatingForm.get('modalFormElegantPassword');
    }
    get modalFormElegantLastName() {
        return this.validatingForm.get('modalFormElegantLastName');
    }
    get modalFormElegantName() {
        return this.validatingForm.get('modalFormElegantName');
    }
}
